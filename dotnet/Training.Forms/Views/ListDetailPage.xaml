<?xml version="1.0" encoding="UTF-8"?>
<d:MvxTabbedPage 
    xmlns:d="clr-namespace:MvvmCross.Forms.Views;assembly=MvvmCross.Forms"
    xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
		xmlns:local="clr-namespace:Training"
		xmlns:core="clr-namespace:Training.Core"
		x:Class="Training.ListDetailPage"
		Title="{Binding PageTitle}">
    <d:MvxTabbedPage.ToolbarItems>
        <ToolbarItem Text="Users" Clicked="OnPageSelect_Clicked" />
        <ToolbarItem Text="+" Clicked="OnAdd_Clicked" />
    </d:MvxTabbedPage.ToolbarItems>
</d:MvxTabbedPage>
